{
    "name": "typical90_extension",
    "short_name": "ty90ex",
    "description": "Chrome extension that makes typical90 easier to use.",
    "manifest_version": 2,
    "version": "1.1.0",
    "author": "taketakeyyy",
    "icons": {
        "16": "icon/icon_x16.png",
        "32": "icon/icon_x32.png",
        "48" : "icon/icon_x48.png",
        "128" : "icon/icon_x128(x96).png"
    },

    "browser_action": {
        "default_title": "typical90_extension",
        "default_icon": {
            "19": "icon/icon_x19.png",
            "38": "icon/icon_x38.png"
        }
    },

    "content_scripts": [
        {
            "js": ["bin/content_scripts.js"],
            "matches": ["http://atcoder.jp/contests/typical90/*", "https://atcoder.jp/contests/typical90/*"],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}